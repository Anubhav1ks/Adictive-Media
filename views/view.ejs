<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="./css/index.css">
    <link rel="stylesheet" href="css/index.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
</head>
<body>
  <nav class="navbar navbar-expand-lg bg-light">
    <div class="container-fluid">
      <!-- <a class="navbar-brand" href="#">Navbar</a> -->
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">

        </ul>
        <form class="d-flex" role="search">
          <!-- <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search"> -->
          <li class="btn btn-outline-success" type="submit"><a href="/">Form</a></li>
        </form>
      </div>
    </div>
  </nav>

  <div class="py-5 text-center">
    <h2>Form Data</h2>
  </div>
 <div class="container form">
    
    <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">DOB</th>
            <th scope="col">Country</th>
            <th scope="col"> Resume</th>
          </tr>
        </thead>
        <tbody>
          <%  datas.forEach(function(data){ %>

            <tr>
              <th scope="row">1</th>
              <td><%=data.firstname+" "+ data.lastname%></td>
              <td><%=data.dob%></td>
              <td><%=data.country %></td>
              <td><a href="./uploads/<%=data.resume%>" target="_blank" rel="noopener noreferrer"><img src="./assest/pdf.png" alt="" height="50px"></a></td>
              <!-- <td><iframe src="./uploads/<%=data.resume%>" style="width: 100%;height: 100%;border: none;"></iframe></td> -->
              <!-- <td><embed src="../uploads/DONBTNGjWbNode test - Addictive Media (1).pdf" width="500" height="375" type="application/pdf"></td> -->
            </tr>
          <% }) %>

        
        </tbody>
      </table>

    </div>


    <script src="js/index.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
</body>
</html>